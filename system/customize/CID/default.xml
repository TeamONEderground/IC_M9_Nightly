<?xml version="1.0" encoding="utf-8"?>
<customization_form>
  <category name="application">
    <module name="FOTA">
      <function name="settings">
        <set name="single">
          <item name="Default_Download_Wifi" type="boolean">yes</item>
        </set>
      </function>
    </module>
    <module name="roaming">
      <function name="default_roaming">
        <set name="plenty" max="5">
          <item name="data_roaming">0</item>
        </set>
      </function>
    </module>
    <module name="OOBE">
      <function name="Step_Config">
        <set name="single">
          <item name="Data_Connection" type="boolean">yes</item>
        </set>
      </function>
      <function name="Data_Connection_Config">
        <set name="single">
          <item name="Show_Mobile" type="boolean">yes</item>
        </set>
      </function>
      <function name="Mobile_Data_Config">
        <set name="single">
          <item name="Default" type="boolean">yes</item>
          <item name="PromptWarningDialog" type="boolean">yes</item>
        </set>
      </function>
    </module>
    <module name="Crash_Report">
      <function name="setting">
        <set name="single">
          <item name="turn_off_report" type="boolean">yes</item>
          <item name="turn_off_application_log" type="boolean">yes</item>
        </set>
      </function>
    </module>
  </category>
  <category name="system">
    <module name="defaultTime1224">
      <function>
        <set name="single">
          <item name="default">24</item>
        </set>
      </function>
    </module>
    <module name="deviceData">
      <function>
        <set name="single">
          <item name="device_name">HTC One M9</item>
        </set>
      </function>
    </module>
